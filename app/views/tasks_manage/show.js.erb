<% if @mobile === "true"%>
    $("#data-table-left .data").remove();
    $('#headOt').remove();
    console.log("in mobile");
    <%if @freeTable === "true"%>        
        $('#data-table-left').append("<%= j render partial: 'user', :locals => {:task => @free_workers} %>");
        console.log("in freeTable");
    <%else%>     
        $('#data-table-left tr').append("<th id='headOt'>OT</th>");
        $('#data-table-left').append("<%= j render partial: 'task', :locals => {:task => @worker_in_department, :mobile => @mobile} %>");
        console.log("in taskTable");    
    <%end%>
    console.log("done table Mobile patial in js.erb"); 
<%else%>
    $("#data-table-left tbody .data").remove();
    $("#data-table-right .data").remove();
    $('#data-table-left tbody').append("<%= j render partial: 'user', :locals => {:task => @free_workers} %>");
    $('#data-table-right').append("<%= j render partial: 'task', :locals => {:task => @worker_in_department} %>");
    console.log("done table patial in js.erb"); 
<%end%>
